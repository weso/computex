prefix wi-people:  <http://data.webfoundation.org/webindex/people/> 
prefix xsd: <http://www.w3.org/2001/XMLSchema#> 


CONSTRUCT {
 ?obs cex:value ?newValue .
} 
WHERE {
 SELECT ?obs ?newValue WHERE {
   ?obs a wi-onto:Observation .
   ?obs cex:value ?value 
   BIND (?value + 1 as ?newValue )
 }
}