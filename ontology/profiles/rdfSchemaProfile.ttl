	 	([ cex:name "RDFS 9" ; 
           cex:uri rdfsURICompute:rdfs9.sparql  
         ]
         [ cex:name "RDFS 11" ; 
           cex:uri rdfsURICompute:rdfs11.sparql  
         ]
        ) ;
   # Integrity queries from RDF Schema
   cex:integrityQuery 
	    [ cex:name "RDFS 9" ;
	      cex:uri	rdfsURIValidate:rdfs9.sparql 
	    ] ; 
   .