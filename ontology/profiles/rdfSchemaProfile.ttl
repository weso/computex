@prefix rdfsURICompute: 	<ontology/computation/rdfSchema/> .
@prefix rdfsURIValidate: 	<ontology/validation/rdfSchema/> .

cex:schema a cex:ValidationProfile ;
   cex:name 			"RDF Schema" ;
